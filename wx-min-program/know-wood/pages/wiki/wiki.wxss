}
.mucaixingtaitezheng .pic_desc_list .wood_img image{
  width: 210rpx;
  height: 210rpx;
  border-radius: 8rpx;
}
.mucaixingtaitezheng .pic_desc_list .wood_name{
  position: absolute;
  width: 100%;
  height: 59rpx;
  bottom: 0;
  left: 0;
}
.mucaixingtaitezheng .pic_desc_list .wood_name .name_bg{
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  border-radius: 8rpx;
}
.mucaixingtaitezheng .pic_desc_list .wood_name .name{
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  color: #fff;
  line-height: 59rpx;
  /* display: flex; */
  align-items: center;
  font-size: 24rpx;
  padding:0 15rpx;
  box-sizing: border-box;
  white-space: nowrap;
  overflow: hidden;
  text-overflow:ellipsis;
}

/* 形态特征end */

