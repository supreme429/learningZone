/* pages/wiki/wiki.wxss */
.container{
  padding: 45rpx 35rpx 0 35rpx
}

/* woodInfo样式start */
.woodInfo .title{
  color: #333;
  text-align: center;
  padding-bottom: 22rpx;
  position: relative;
  display: flex;
  justify-content: center;
  margin-bottom: 30rpx;
}
.woodInfo .icon-title-arrow{
  position: absolute;
  width: 18rpx;
  height: 14rpx;
  bottom: 0;
  left: 50%;
  transform: translateX(-50%)
}
.woodInfo .title-txt{
  position: relative;
  font-size: 36rpx;
  height: 36rpx;
  line-height: 36rpx;
  width: auto;
  font-weight: bold;
}
.woodInfo .woodImg image{
  width: 100%;
  height: 410rpx;
  border-radius: 10rpx;
  display: block;
}
/* woodInfo样式end */

/* liangdianxinxi样式start */
.liangdianxinxi{
  margin-top: 25rpx;
}
.liangdianxinxi .xinxi-item{
  display: flex;
  font-size: 28rpx;
  line-height: 28rpx;
  color: #666
}
.liangdianxinxi .xinxi-item + .xinxi-item{
  margin-top: 20rpx;
}
.liangdianxinxi .xinxi-item .xinxi-info{
  margin-left: 20rpx;
}
/* liangdianxinxi样式end */

/* 信息title start*/

.xinxi_title{
  display: flex;
  align-items: center;
  font-size: 30rpx;
  color: #333;
  margin-bottom: 20rpx;
  margin-top: 50rpx;
}
.xinxi_title .icon-title-arrow{
  display: inline-block;
  width: 18rpx;
  height: 14rpx;
  transform: rotate(-90deg);
  margin-right: 10rpx;
}

.jibenxinxi .xinxi-item {
  font-size: 28rpx;
  line-height: 28rpx;
  color: #666;
  margin-bottom: 20rpx;
}
.jibenxinxi .xinxi-item .xinxi-name{
  margin-right: 20rpx;
}
.jibenxinxi .xinxi-item text{
  display: inline-block;
}
.jibenxinxi .xinxi-item image{
  width: 100%;
}
.jibenxinxi .xinxi-item video{
  width: 100%;
}
/* 信息title end*/

/* 优缺点start */
.youdianjiquedian .xinxi-item {
  font-size: 28rpx;
  color: #666;
}
.youdianjiquedian .xinxi-item .xinxi-info{
  line-height: 50rpx
}
.youdianjiquedian .xinxi-item .xinxi-name{
  display: block;
  margin-right: 20rpx;
}
.youdianjiquedian .xinxi-item text{
  display: inline-block;
}
/* 优缺点end */
.xinxi-item image{
  display: block;
  margin-top: 20rpx;
}
/* 常见家居展示start */
.woodGoodsInfo .wrap-swiper{
  margin-top: 18rpx;
}
.woodGoodsInfo .wrap-swiper swiper-item{
  padding: 0 20rpx;
  box-sizing: border-box;
  width: 400rpx;
  overflow: hidden;
}
.woodGoodsInfo .wrap-swiper swiper-item image{
  width: 100%;
  height: 100%;
  border-radius: 8rpx;
}
.woodGoodsInfo swiper-item .woodName{
  position: absolute;
  bottom: 0;
  height: 52rpx;
  width: 100%;
  left: 0rpx;
  padding: 0 20rpx;
  box-sizing: border-box;
}
.woodGoodsInfo swiper-item .woodName image{
  border-radius: 8rpx;
}
.woodGoodsInfo swiper-item .woodName text{
  position: absolute;
  left: 40rpx;
  top: 15rpx;
  color: #fff;
  font-size: 24rpx;
}
.woodGoodsInfo .indicator-dots{
  display: flex;
  justify-content: center;
  margin-top: 20rpx;
}
.woodGoodsInfo .indicator-dots .dots{
  width: 10rpx;
  height: 10rpx;
  background-color: #e8e8e8;
  border-radius: 10rpx;
  margin-left: 10rpx;
}
.woodGoodsInfo .indicator-dots .dots.active{
  background-color: #e1c195
}

/* 常见家居展示end */
/* 加工性start */
.jiagongxingheyongtu .xinxi-item {
  font-size: 28rpx;
  color: #666;
}
.jiagongxingheyongtu .xinxi-item .xinxi-info{
  line-height: 50rpx
}
.jiagongxingheyongtu .xinxi-item .xinxi-name{
  margin-right: 20rpx;
}
.jiagongxingheyongtu .xinxi-item text{
  display: inline-block;
}
.jiagongxingheyongtu .xinxi-item image{
  width: 100%;
}
/* 加工性end */

/* 使用历史start */
.shiyonglishijieshao .pic_list{
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
}
.shiyonglishijieshao .pic_list .pic_item{
  font-size: 0;
  margin-top: 40rpx;
}
.shiyonglishijieshao .pic_list .pic_item image{
  width:320rpx;
  height:240rpx;
  border-radius: 8rpx;
}

/* 分布范围start */
.desc{
  font-size: 28rpx;
  color: #666;
  line-height: 45rpx;
}
/* 分布范围end */

/* 形态特征start */
.xingtai_img image{
  width: 100%;
}
.mucaixingtaitezheng .xinxi-list{
  font-size: 28rpx;
  color: #666
}
.mucaixingtaitezheng .xinxi-list .xinxi-name{
  margin-right: 20rpx;
}
.mucaixingtaitezheng .xinxi-list text{
  display: inline-block;
  line-height: 45rpx;
}
.mucaixingtaitezheng .pic_desc_list{
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
  margin-top: 30rpx;
}
.mucaixingtaitezheng .pic_desc_list .pic_item{
  width: 210rpx;
  height: 210rpx;
  overflow: hidden;
  position: relative;
  border-radius: 8rpx;
