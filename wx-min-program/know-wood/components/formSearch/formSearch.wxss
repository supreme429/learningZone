/* components/formSearch/formSearch.wxss */
.container{
  width: 100%;
  display: flex;
  padding: 20rpx 35rpx 0 35rpx;
  justify-content: space-between;
  box-sizing: border-box;
}
.container .input-box{
  width: 588rpx;
  background-color: #f8f8f8;
  height: 90rpx;
  border-radius: 90rpx;
  display: flex;
  position: relative;
}
.container .input-box .icon-search{
  height: 90rpx;
  width: 63rpx;
  display: flex;
  justify-content: flex-end;
  align-items: center
}
.container .input-box .icon-search image{
  width: 33rpx;
  height: 33rpx;
}
.container .input-box input{
  padding-left: 22rpx;
  background-color: #f8f8f8;
  height: 90rpx;
  font-size: 28rpx;
  border-radius: 90rpx
}
.container .btn-search{
  width: 100rpx;
  margin: 0;
  padding: 0;
  height: 90rpx;
  line-height: 90rpx;
  font-size: 28rpx;
  color: #f57724;
  background: #fff
}
.container .btn-search.noTab{
  color: #ccc
}
.input-box .icon-delete{
  width: 34rpx;
  height: 34rpx;
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
  right: 30rpx;
  z-index: 99;
}
.input-box .icon-delete .img{
  width: 34rpx;
  height: 34rpx;
}

button::after{
  border: 0;
}