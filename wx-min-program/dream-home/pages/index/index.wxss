/* miniprogram/pages/index/index.wxss */
.logo{
  display: block;
  width: 135rpx;
  height: 137rpx;
  margin: 40rpx auto 0;
}
.mina-title{
  display: block;
  width: 357rpx;
  height: 121rpx;
  margin: 24rpx auto 0;
}
.text-title{
  font-size: 30rpx;
  color: #333;
  text-align: center;
  margin-top: 40rpx;
}
.content-img{
  display: block;
  width: 400rpx;
  height: 400rpx;
  margin: 40rpx auto 0;
}
.btn-box{
  position: relative;
  width: 480rpx;
  height: 130rpx;
  margin: 50rpx auto 0;
}
.btn-box image,
.btn-box button{
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
}
.tips{
  text-align: center;
  color: #999;
  font-size: 24rpx;
  margin-top: 37rpx;
}