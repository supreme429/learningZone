/* components/dialogMoveBargain/dialogMoveBargain.wxss */
.container{
  position: fixed;
  background-color: rgba(0, 0, 0, 0.5);
  width: 100%;
  height: 100%;
  top: 0;
  left: 0
}
.container .content{
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translate(-50%,-50%);
  background-color: #fff;
  padding: 40rpx 0 70rpx 0;
  border-radius: 12rpx;
}
.container .helpBargain image{
  width: 500rpx;
  height: 400rpx;
}
.container .icon-result{
  width: 500rpx;
  height: 400rpx;
  font-size: 0;
  text-align: center;
  position: relative
}
.container .firstBargain{
  width: 580rpx;
  height: 208rpx;
}
.container .icon-result image{
  width: 312rpx;
  height: 307rpx;
}
.container .firstBargain .img-firstBargain{
  width: 580rpx;
  height: 291rpx;
  position: absolute;
  top: -125rpx;
  left: 0
}

.container .helpInfo{
  font-size: 34rpx;
  text-align: center;
}
.container .helpInfo .price{
  color: #e62318
}