page {
  font-family: PingFangSC-Light,'Helvetica Neue',Helvetica,sans-serif;
}

.layer-mask{
  position: fixed;
  z-index: 999;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  background: rgba(0, 0, 0, 0.4);
}
.layer-container {
  width: 80%;
  background-color: #fff;
  position: fixed;
  z-index: 1000;
  top: 20%;
  left: 10%;
  border-radius: 10rpx;
}
.layer-container .title {
  display: flex;
  justify-content: flex-end;
}
.layer-container .title .close {
  padding: 15rpx;
  width: 30rpx;
  height: 30rpx; 
}
.layer-container .title .close text {
  display: block;
  width: 30rpx;
  height: 30rpx;  
  background: url(data:image/gif;base64,) center center no-repeat;
  background-size: contain;
}
.layer-container .content {
  padding: 0 100rpx 55rpx;
}
.layer-container .content .row-msg {
  display: flex;
}
.layer-container .content .icon {
  width: 95rpx;
  height: 95rpx;
  margin-right: 20rpx;
  background-position: center center;
  background-repeat: no-repeat;
}
.layer-container .content .icon.ok {
  background-image: url(data:image/png;base64,);
  background-size: contain;
}
.layer-container .content .icon.warn {
  background-image: url(data:image/png;base64,);
  background-size: contain;
}
.layer-container .content .msg {
  flex: 1;
}
.layer-container .content .msg-text {
  font-size: 36rpx;
  color: #333;
}
.layer-container .content .msg-note {
  font-size: 26rpx;
  color: #999;
  margin-top: 15rpx;
}
.layer-container .content .sure {
  height: 80rpx;
  background-color: #ff7849;
  color: #fff;
  font-size: 30rpx;
  line-height: 80rpx;
  border: none;
  border-radius: 40rpx;
  font-family: PingFangSC-Light,'Helvetica Neue',Helvetica,sans-serif;
  margin-top: 80rpx;
}

